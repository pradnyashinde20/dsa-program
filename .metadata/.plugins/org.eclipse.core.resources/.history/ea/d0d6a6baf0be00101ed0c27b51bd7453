package com.demo.test;

import java.util.Scanner;

import com.demo.list.MyGraph;

public class TestAdjecencyList {

	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("how many vertices");
	int v= sc.nextInt();
	MyGraph g = new MyGraph(v);
	
	storedGraph(g,v);
	g.printGraph();
	g.dfsTraversal(0);

	}

	private static void storedGraph(MyGraph g, int v) {
	Scanner sc = new Scanner(System.in);
	for(int i=0;i<v;i++) {
		for(int j=0;j<v;j++) {
			System.out.println("is there edge exist between"+i+' '+j);
			int val=sc.nextInt();
			if(val>0) {
				g.addEdge(i, j);
			}
		}
	}
		
	}

}
