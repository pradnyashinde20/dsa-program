package com.demo.linkedlist;

public class DoublyLinkedList2 {
Node head;
class Node{
	int data;
	Node prev;
	Node next;
	public Node(int data)
	{
		this.data=data;
		prev=null;
		next=null;
	}
}
public void addNewNode(int val)
{
Node newNode=new Node(val);
if(head==null)
{
	head=newNode;
}
else {
	Node temp;
	temp=head;
	while(temp!=null)
	{
		temp=temp.next;
	}
	newNode.prev=temp;
	temp.next=newNode;
}
}

public void display()
{
	Node temp=head;
	if(head==null)
	{
		System.out.println("list is empty");
	}
	else {
		System.out.print(temp.data+" ---->");
	}
	
}
}
