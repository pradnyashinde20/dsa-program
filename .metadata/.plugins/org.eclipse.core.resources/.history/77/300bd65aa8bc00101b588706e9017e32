package com.queue.service;

public class CircularQueue {
	
	int arr[];
	int front;
	int rear;
	
	public CircularQueue() {
		arr=new int[10];
		this.front = -1;
		this.rear=-1;
	}
	
	public CircularQueue(int size) {
		arr=new int[size];
		this.front = -1;
		this.rear=-1;
	}
	
  public boolean isEmpty() {
	  if(front ==-1) {
		  System.out.println("queue is empty");
		  return true;
	  }
	  return false;
  }

}
